{
  "name": "chargeCreated",
  "type": "object",
  "properties": {
    "chargeUuid": {
      "type": "string",
      "description": "Unique ID for the charge that has been created"
    },
    "chargeIntent": {
      "type": "string",
      "enum": [
        "METHOD_ADD",
        "METHOD_REFRESH",
        "METHOD_VERIFY",
        "CAPTURE",
        "REFUND"
      ],
      "description": "The intent of this charge"
    },
    "contract": {
      "enum": [
        "NONE",
        "PAYMENT",
        "SUBSCRIPTION_INITIAL",
        "SUBSCRIPTION_RENEWAL",
        "ONECLICK"
      ]
    },
    "amount": {
      "$ref": "money.json"
    },
    "references": {
      "type": "object",
      "description": "A keyed array of references assigned to the charge"
    },
    "expiryTime": {
      "type": "number",
      "description": "Expiry time of the charge, output as the number of seconds since unix epoch"
    },
    "merchantReference": {
      "type": "string",
      "description": "Unique reference for this charge, as defined by the merchant"
    },
    "References": {
      "type": "object",
      "description": "References for this transaction"
    }
  }
}
