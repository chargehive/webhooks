{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "name": "responseDetail",
  "type": "object",
  "properties": {
    "responseCode": {
      "type": "string",
      "description": "Return code generated by ChargeHive"
    },
    "consumerMessage": {
      "type": "string",
      "description": "Consumer safe message"
    },
    "merchantMessage": {
      "type": "string",
      "description": "Internal message for merchants to use"
    },
    "failureType": {
      "enum": [
        "NONE",
        "SOFT",
        "HARD",
        "RETRY",
        "INTERNAL"
      ]
    },
    "originatingCode": {
      "type": "string",
      "description": "Original response code, usually generated by a connector"
    }
  }
}
